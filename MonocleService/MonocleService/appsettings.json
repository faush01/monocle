{
	"DbServer": {
		"DbFileName": "metrics.db",
		"Retention": {
			"test": 20,
			"test_event_table": 20,
			"events": 14,
			"environment": 30,
			"power_usage": 180
		}
	},
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://*:3456"
			}
		}
	},
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
	"AllowedHosts": "*"
}
